{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "env": {
    "coverage": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "react"
      ],
      "plugins": [
        [
          "istanbul",
          {
            "exclude": [
              "test/**/*.js"
            ]
          }
        ]
      ]
    }
  }
}
